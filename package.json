    "@blueprintjs/table": "^3.8.17",
    "@types/jest": "^24.0.11",
    "@types/js-beautify": "^1.8.1",
    "@types/papaparse": "^5.0.4",
    "@types/react": "^16.8.7",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^5.1.5",
    "codemirror": "^5.48.0",
    "js-beautify": "^1.10.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.0.0",
    "normalize.css": "^8.0.1",
    "papaparse": "^5.2.0",
    "react": "^16.8.4",
    "react-codemirror2": "^6.0.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "~3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "bumpversion:patch": "npm version patch && yarn build:version",
    "bumpversion:prerelease": "npm version prerelease --preid=rc && yarn build:version",
    "build": "react-scripts build",
    "build:launcher-only": "rimraf launcher/am4editor && mkdirp launcher/am4editor && cpr build launcher/am4editor && cd launcher && yarn build",
    "build:launcher": "yarn build:version && react-scripts build && yarn build:launcher-only",
    "build:version": "ts-node --project ./scripts/version/tsconfig.json ./scripts/version/imprint-version",
    "build:zip": "ts-node --project ./scripts/zip-release/tsconfig.json ./scripts/zip-release/zip-release",
    "build:deploy": "react-scripts build && mkdir deploy && mkdir deploy/am4editor && cp -r build/* deploy/am4editor",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "adm-zip": "^0.4.14",
    "cpr": "^3.0.1",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "mobx-react-devtools": "^6.1.1",
    "prettier": "^1.17.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1"
  }
}
